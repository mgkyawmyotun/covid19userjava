<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXSpinner?>
<?import com.jfoenix.controls.JFXTreeTableView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="main" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.GlobalTableController">
    <children>
        <BorderPane fx:id="borderPane" prefHeight="565.0" prefWidth="653.0" stylesheets="@../css/stateView.css">
            <center>
                <StackPane BorderPane.alignment="CENTER">
                    <BorderPane.margin>
                        <Insets />
                    </BorderPane.margin>
                    <children>
                        <JFXTreeTableView fx:id="treeView" prefHeight="38.0" prefWidth="706.0" stylesheets="@../css/tableDesign.css">
                            <padding>
                                <Insets left="100.0" right="100.0" />
                            </padding>
                            <columnResizePolicy>
                                <TreeTableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                            </columnResizePolicy>
                        </JFXTreeTableView>
                        <JFXSpinner fx:id="tableLoading" radius="50.0" visible="false" />
                    </children>
                </StackPane>
            </center>
         <top>
            <AnchorPane prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #424242;" BorderPane.alignment="CENTER" />
         </top>
        </BorderPane>
    </children>
</StackPane>
